#include "core/basic.emi";
#include "core/print.emi";

fibonacci :: (i : s64) -> s64
{
	a := 0;
	b := 1;
	while i >= 0
	{
		t := a + b;
		a = b;
		b = t;
		i = i - 1;
	}
	return b;
}

Main :: (args : [] String) -> s64
{
	for (0..8) PrintF("fibonacci(%): %\n", i, fibonacci(i));

	Print("Test ok\n");
	return 0;
}

argByPointer :: (n : ^s32)
{
	@n = @n * @n;
}

Main :: (args : [] String) -> s64
{
	foo : s32 = 5;
	bar : s32;
	baz : ^s32;

	baz = ^foo;
	bar = @baz;

	PrintF("Pointer to foo: %\n", baz);

	a : ^^s32 = ^baz;
	c : s32 = @@a;

	// Pointer to temporal value
	//b : ^^s32 = ^^bar;

	// Diferent types
	//d : s8 = @baz;
	//e : ^s8 = ^foo;

	argByPointer(^foo);

	aStruct : struct { a : s32; };
	ptrToMember := ^aStruct.a;
	@ptrToMember = 42;
	PrintF("Pointer to struct member: %\n", ptrToMember);

	Print("Test ok\n");
	return 0;
}

#include "core/basic.fab";
#include "core/print.fab";
#linklib "output/lib";

LibAddTwoNumbers :: #external (a : s64, b : s64) -> s64;

Main :: (args : [] String) -> s64
{
	sum := LibAddTwoNumbers(1, 2);
	PrintF("1 + 2 is %, says linked library.\n", sum);
	if sum != 3 return 1;

	Print("Test ok\n");
	return 0;
}

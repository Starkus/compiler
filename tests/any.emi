Proc :: (param : Any)
{
	if param.typeInfo.typeKind == TypeKind_Integer
	{
		msg : String = "An integer of size %\n";
		msg[msg.size - 3] = cast(u8) param.typeInfo.size + '0';
		Print(msg);
	}
	else if param.typeInfo.typeKind == TypeKind_Floating
	{
		msg : String = "An float of size %\n";
		msg[msg.size - 3] = cast(u8) param.typeInfo.size + '0';
		Print(msg);
	}
}

Main :: () -> s64
{
	n : s64 = 2345;
	a : Any;
	a.typeInfo = typeof(n);
	a.data = ^n;
	Proc(a);

	anAny : Any = n;

	Proc(12345.6789);

	Print("Test ok\n");
	return 0;
}

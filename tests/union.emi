Test :: union
{
	integer : s32;
	float : f32;
	struct
	{
		a : s64;
		b : s64;
	};
};

Main :: (args : [] String) -> s64
{
	test : Test;
	test.float = 2.0;
	asInt : s32 = test.integer;
	a := test.a;
	b := test.b;

	PrintF("Union: { int:%, float:%, struct:{%, %} }\n", test.integer, test.float, test.a, test.b);

	Print("Test ok\n");
	return 0;
}

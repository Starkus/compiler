Person :: struct
{
	id : u64;
	age : s32;
	height : f32;
}

Couple :: struct
{
	husband : Person;
	wife : Person;
}

structAsArgument :: (person : ^Person)
{
	foo := person.age;
}

main :: ()
{
	a : Person;
	b : Person;
	
	a.id = 123456789;
	a.age = 25;
	a.height = 170;

	// Copy
	b = a;

	couple : Couple;
	couple.husband.id = 40;
	couple.husband.age = 22;
	couple.husband.height = 1.73;
	couple.wife.id = 41;
	couple.wife.age = 29;
	couple.wife.height = 1.71;

	structAsArgument(^a);
}

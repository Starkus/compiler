AnonReturn :: () -> struct { one : s64; two : s32; }
{
	return { 1, 2 };
}

AnonParam :: (param : struct { three : s64; four : s64; })
{
	PrintF("Anonymous struct parameter received: %\n", param);
}

Main :: () -> s64
{
	struct
	{
		foo : s32;
		bar : s32;
	};
	foo = bar;

	aStruct : struct
	{
		a : f32;
	};
	aStruct.a = 5.0;

	union
	{
		asInt : s64;
		asFloat : f64;
	};
	asFloat = 2.5;

	struct
	{
		struct
		{
			struct
			{
				deep : s64;
			};
		};
	};
	deep = 5;

	anonReturnValue := AnonReturn();
	PrintF("Anonymous struct return: %\n", anonReturnValue);

	AnonParam({ 3, 4 });

	Print("Test ok\n");
	return 0;
}

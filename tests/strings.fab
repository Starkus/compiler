#include "core/basic.fab";
#include "core/print.fab";

Main :: (args : [] String) -> s64
{
	str := "Hello, Sailor!";
	PrintF("String is \"%\"\n", str);

	i := 1;
	strPtr := ^str;
	char : u8 = str[i];

	str[2] = '?';
	PrintF("String is \"%\"\n", str);

	for (str) @it = cast(u8)('0'+i);
	PrintF("String is \"%\"\n", str);

	Print("Test ok\n");
	return 0;
}
